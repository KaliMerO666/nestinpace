  "scripts": {
    "build:datocms": "dato dump",
    "build:hugo": "hugo --config config.prod.toml --gc --minify --verbose",
    "build": "npm run build:datocms && npm run build:hugo",
    "dev": "npm run build:datocms && hugo server --config config.dev.toml"
  },
  "devDependencies": {
    "datocms-cli": "^1.0.12"
  }
